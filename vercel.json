{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "USE_SUPABASE": "true",
    "SKIP_SAMPLE_DATA": "true"
  }
} 