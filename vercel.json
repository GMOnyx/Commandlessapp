{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/welcome",
      "destination": "/index.html"
    },
    {
      "source": "/(sign-in|sign-up|dashboard|mappings|mappings/.*|connections)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "https://commandless.vercel.app/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "env": {
    "VITE_CLERK_PUBLISHABLE_KEY": "pk_live_Y2xlcmsuY29tbWFuZGxlc3MuYXBwJA"
  }
} 